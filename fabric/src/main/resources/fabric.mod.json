{
    "schemaVersion": 1,
    "id": "annotateddi",
    "version": "${version}",
    "name": "${readable_name}",
    "description": "${mod_description}",
    "authors": [
        "The_Fireplace"
    ],
    "contact": {
        "homepage": "${modrinth_url}",
        "sources": "${github_url}",
        "issues": "${github_url}/issues",
        "curseforge": "${fabric_curseforge_url}"
    },
    "license": "${license_id}",
    "icon": "${logo_filename}",
    "environment": "*",
    "entrypoints": {
        "main": [
            "dev.the_fireplace.annotateddi.impl.entrypoint.Main"
        ],
        "client": [
            "dev.the_fireplace.annotateddi.impl.entrypoint.Client"
        ],
        "server": [
            "dev.the_fireplace.annotateddi.impl.entrypoint.Server"
        ]
    },
    "depends": {
        "fabricloader": ">=0.12.0",
        "minecraft": ">=1.20"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "${discord_url}",
                "annotateddi.twitter": "${twitter_url}",
                "annotateddi.kofi": "${kofi_url}"
            },
            "badges": [
                "library"
            ]
        }
    }
}
